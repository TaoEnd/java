<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <servlet>
    <servlet-name>hello-servlet</servlet-name>
    <servlet-class>com.bjpowernode.servlets.HelloServlet</servlet-class>
    <load-on-startup>3</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>hello-servlet</servlet-name>
    <url-pattern>/some</url-pattern>
    <url-pattern>/some2</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>OtherMethods</display-name>
    <servlet-name>OtherMethods</servlet-name>
    <servlet-class>com.bjpowernode.servlet.OtherMethods</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>OtherMethods</servlet-name>
    <url-pattern>/other</url-pattern>
  </servlet-mapping>
</web-app>